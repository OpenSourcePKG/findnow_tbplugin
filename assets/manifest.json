{
  "applications": {
    "gecko": {
      "id": "FindNow@hw-softwareentwicklung.de",
      "strict_max_version": "115.*",
      "strict_min_version": "103.0"
    }
  },
  "author": "Stefan Werfling",
  "background": {
    "scripts": [
      "chrome/background.js"
    ]
  },
  "default_locale": "de",
  "description": "__MSG_extensionDescription__",
  "experiment_apis": {
    "findnow": {
      "parent": {
        "events": [
          "startup"
        ],
        "paths": [
          [
            "findnow"
          ]
        ],
        "scopes": [
          "addon_parent"
        ],
        "script": "chrome/api/findnow/implementation.js"
      },
      "schema": "chrome/api/findnow/schema.json"
    }
  },
  "icons": {
    "64": "chrome/content/images/findnow.png"
  },
  "manifest_version": 2,
  "message_display_action": {
    "default_title": "__MSG_message_display_action_default_title__",
    "type": "button"
  },
  "name": "__MSG_extensionName__",
  "options_ui": {
    "browser_style": false,
    "open_in_tab": true,
    "page": "chrome/content/ui/options.html"
  },
  "permissions": [
    "accountsRead",
    "compose",
    "idle",
    "messagesDelete",
    "messagesModify",
    "messagesMove",
    "messagesRead",
    "storage",
    "tabs",
    "theme"
  ],
  "version": "2.7"
}